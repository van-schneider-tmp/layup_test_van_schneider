{
    "extends": [
      "react-app", 
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["react", "prettier"],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": "error",
      "react/react-in-jsx-scope": "off" 
    }
  }
  